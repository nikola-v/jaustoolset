<?xml version="1.0" encoding="UTF-8"?>
<declared_type_set xmlns="urn:jaus:jsidl:1.0" name="InformClass"
	id="urn:jaus:jss:environmentSensing:MessageSet:InformClass"
	version="1.0">
	<declared_const_set_ref name="constants"
		id="urn:jaus:jss:environmentSensing:MessageSet:BasicConstants"
		version="1.0"/>
	<declared_type_set_ref name="coreTypes"
		id="urn:jaus:jss:core:MessageSet:BasicTypes" version="1.0"/>
	<declared_type_set_ref name="environmentTypes"
		id="urn:jaus:jss:environmentSensing:MessageSet:BasicTypes"
		version="1.0"/>

	<message_def name="ReportRangeSensorCapabilities"
		message_id="4801" is_command="false">
		<description xml:space="preserve">This message is used to report the 
            range sensorsâ€™ capabilities upon reciept of a Query Range Sensor 
            Capabilities message. Capabilities include both static sensor 
            properties and valid values and ranges for properties which 
            can be modified by the Set Range Sensor Capabilities Message.</description>
		<declared_header name="AppHeader"
			declared_type_ref="coreTypes.JAUSApplicationLayerHeader"/>
		<body name="Body">
			<list name="RangeSensorCapabilitiesList" optional="false">
				<count_field min_count="0" max_count="65535"
					field_type_unsigned="unsigned short integer"/>
				<record name="RangeSensorCapabilitiesRec"
					optional="false">
					<presence_vector
						field_type_unsigned="unsigned short integer"/>
					<declared_fixed_field name="SensorID"
						declared_type_ref="environmentTypes.SensorID"
						optional="false"/>
					<declared_variable_length_string name="SensorName"
						declared_type_ref="environmentTypes.SensorName"
						optional="false"/>
					<declared_bit_field name="SupportedStates"
						declared_type_ref="environmentTypes.SupportedSensorStates"
						optional="true"/>
					<declared_fixed_field
						name="MinimumHorizontalFieldOfViewStartAngle"
						declared_type_ref="environmentTypes.FieldOfView"
						optional="true"/>
					<declared_fixed_field
						name="MaximumHorizontalFieldOfViewStopAngle"
						declared_type_ref="environmentTypes.FieldOfView"
						optional="true"/>
					<declared_fixed_field
						name="MinimumVerticalFieldOfViewStartAngle"
						declared_type_ref="environmentTypes.FieldOfView"
						optional="true"/>
					<declared_fixed_field
						name="MaximumVerticalFieldOfViewStopAngle"
						declared_type_ref="environmentTypes.FieldOfView"
						optional="true"/>
					<declared_fixed_field name="MiniumumUpdateRate"
						declared_type_ref="environmentTypes.SensorUpdateRate"
						optional="true"/>
					<declared_fixed_field name="MaximumUpdateRate"
						declared_type_ref="environmentTypes.SensorUpdateRate"
						optional="true"/>
					<declared_fixed_field name="MinimumRange"
						declared_type_ref="environmentTypes.SensorRange"
						optional="true"/>
					<declared_fixed_field name="MaximumRange"
						declared_type_ref="environmentTypes.SensorRange"
						optional="true"/>
					<bit_field name="SupportedCompression"
						optional="true"
						field_type_unsigned="unsigned byte">
						<sub_field name="NoCompression">
							<bit_range from_index="0" to_index="0"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="DEFLATE">
							<bit_range from_index="1" to_index="1"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="bzip2">
							<bit_range from_index="2" to_index="2"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="LZMA">
							<bit_range from_index="3" to_index="3"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
					</bit_field>
					<fixed_field
						name="CoordinateTransformationSupported"
						optional="true" field_units="one"
						field_type="unsigned byte">
						<value_set offset_to_lower_limit="false">
							<value_enum enum_index="0"
								enum_const="False"/>
							<value_enum enum_index="1"
								enum_const="True"/>
						</value_set>
					</fixed_field>
				</record>
			</list>
		</body>
		<footer name="Footer"/>
	</message_def>

	<message_def name="ReportRangeSensorConfiguration"
		message_id="4802" is_command="false">
		<description xml:space="preserve">This message is sent in response 
            to a Query Range Sensor Configuration message. It is populated 
            with the current sensor configuration (per sensor ID) as defined 
            in the table below.</description>
		<declared_header name="AppHeader"
			declared_type_ref="coreTypes.JAUSApplicationLayerHeader"/>
		<body name="Body">
			<list name="RangeSensorConfigurationList" optional="false">
				<count_field min_count="0" max_count="65535"
					field_type_unsigned="unsigned short integer"/>
				<declared_record name="RangeSensorConfigurationRec"
					declared_type_ref="environmentTypes.RangeSensorConfigurationRec"
					optional="false"/>
			</list>
		</body>
		<footer name="Footer"/>
	</message_def>

	<message_def name="ReportRangeSensorData" message_id="4803"
		is_command="false">
		<description xml:space="preserve">This message is sent by a receiving 
            component upon receipt of a Query Range Sensor Data message. This 
            message reports a list of detected data points for a given time. 
            The RangeSensorDataSeq is used to report the data from a given 
            sensor along with meta data such as sensor ID and timestamp. The 
            timestamp defines the time at which the collected data was valid. 
            Data points are defined by range, bearing and inclination with 
            respect to either the native coordinate system or the vehicle 
            coordinate system. Data is only reported for sensors which are 
            in the active state. If data is queried for a sensor which is 
            not active, the RangeSensorDataErrorRec is returned for that 
            sensor ID report.</description>
		<declared_header name="AppHeader"
			declared_type_ref="coreTypes.JAUSApplicationLayerHeader"/>
		<body name="Body">
			<list name="RangeSensorDataList" optional="false">
				<count_field min_count="0" max_count="65535"
					field_type_unsigned="unsigned short integer"/>
				<variant name="RangeSensorDataVariant"
					optional="false">
					<vtag_field min_count="0" max_count="1"
						field_type_unsigned="unsigned byte"/>
					<declared_record name="RangeSensorDataErrorRec"
						declared_type_ref="environmentTypes.RangeSensorDataErrorRec"
						optional="false"/>
					<sequence name="RangeSensorDataSeq"
						optional="false">
						<record name="RangeSensorDataRec"
							optional="false">
							<declared_fixed_field name="SensorID"
								declared_type_ref="environmentTypes.SensorID"
								optional="false"/>
							<declared_fixed_field
								name="ReportCoordinateSystem"
								declared_type_ref="environmentTypes.CoordinateSystemEnum"
								optional="false"/>
							<declared_bit_field name="TimeStamp"
								declared_type_ref="coreTypes.TimeStamp"
								optional="false"/>
						</record>
						<list name="RangeSensorDataPointList"
							optional="false">
							<count_field min_count="0"
								max_count="65535"
								field_type_unsigned="unsigned short integer"/>
							<record name="RangeSensorDataPointRec"
								optional="false">
								<presence_vector
								field_type_unsigned="unsigned short integer"/>
								<fixed_field name="PointID"
								optional="true" field_units="one"
								field_type="unsigned integer"/>
								<declared_fixed_field name="Range"
								declared_type_ref="environmentTypes.SensorRange"
								optional="false"/>
								<fixed_field name="RangeValidity"
								optional="true" field_units="one"
								field_type="unsigned byte">
								<value_set
								offset_to_lower_limit="false">
								<value_enum enum_index="0"
								enum_const="False"/>
								<value_enum enum_index="1"
								enum_const="True"/>
								</value_set>
								</fixed_field>
								<fixed_field name="RangeErrorRMS"
								optional="true" field_units="meter"
								field_type="unsigned integer">
								<scale_range real_lower_limit="0.0"
								real_upper_limit="100000.0"
								integer_function="round"/>
								</fixed_field>
								<declared_fixed_field name="Bearing"
								declared_type_ref="environmentTypes.FieldOfView"
								optional="false"/>
								<fixed_field name="BearingValidity"
								optional="true" field_units="one"
								field_type="unsigned byte">
								<value_set
								offset_to_lower_limit="false">
								<value_enum enum_index="0"
								enum_const="False"/>
								<value_enum enum_index="1"
								enum_const="True"/>
								</value_set>
								</fixed_field>
								<fixed_field name="BearingErrorRMS"
								optional="true"
								field_units="radian"
								field_type="unsigned integer">
								<scale_range real_lower_limit="0"
								real_upper_limit="constants.PI"
								integer_function="round"/>
								</fixed_field>
								<declared_fixed_field
								name="Inclination"
								declared_type_ref="environmentTypes.FieldOfView"
								optional="false"/>
								<fixed_field
								name="InclinationValidity"
								optional="true" field_units="one"
								field_type="unsigned byte">
								<value_set
								offset_to_lower_limit="false">
								<value_enum enum_index="0"
								enum_const="False"/>
								<value_enum enum_index="1"
								enum_const="True"/>
								</value_set>
								</fixed_field>
								<fixed_field
								name="InclinationErrorRMS"
								optional="true"
								field_units="radian"
								field_type="unsigned integer">
								<scale_range real_lower_limit="0.0"
								real_upper_limit="constants.PI"
								integer_function="round"/>
								</fixed_field>
							</record>
						</list>
					</sequence>
				</variant>
			</list>
		</body>
		<footer name="Footer"/>
	</message_def>

	<message_def name="ReportRangeSensorCompressedData"
		message_id="4804" is_command="false">
		<description xml:space="preserve">This message is used to report a 
            list of detected data points for a given time. This message differs 
            from the Report Range Sensor Data message in that the data contained 
            herein has undergone compression by the specified algorithm. The 
            timestamp defines the time at which the collected data was valid. 
            Data points are defined by range, bearing and inclination with 
            respect to either the sensor coordinate system or a specified 
            coordinate system.</description>
		<declared_header name="AppHeader"
			declared_type_ref="coreTypes.JAUSApplicationLayerHeader"/>
		<body name="Body">
			<list name="RangeSensorCompressedDataList"
				optional="false">
				<count_field min_count="0" max_count="65535"
					field_type_unsigned="unsigned short integer"/>
				<variant name="RangeSensorCompressedDataVariant"
					optional="false">
					<vtag_field min_count="0" max_count="1"
						field_type_unsigned="unsigned byte"/>
					<declared_record name="RangeSensorDataErrorRec"
						declared_type_ref="environmentTypes.RangeSensorDataErrorRec"
						optional="false"/>
					<record name="RangeSensorCompressedDataRec"
						optional="false">
						<declared_fixed_field name="SensorID"
							declared_type_ref="environmentTypes.SensorID"
							optional="false"/>
						<declared_fixed_field
							name="ReportCoordinateSystem"
							declared_type_ref="environmentTypes.CoordinateSystemEnum"
							optional="false"/>
						<declared_bit_field name="TimeStamp"
							declared_type_ref="coreTypes.TimeStamp"
							optional="false"/>
						<declared_fixed_field name="DataCompression"
							declared_type_ref="environmentTypes.DataCompressionEnum"
							optional="false"/>
						<variable_length_field name="CompressedData"
							optional="false"
							field_format="RangeSensorDataPointList">
							<count_field min_count="0"
								max_count="4294967295"
								field_type_unsigned="unsigned integer"
							/>
						</variable_length_field>
					</record>
				</variant>
			</list>
		</body>
		<footer name="Footer"/>
	</message_def>

	<message_def name="ReportSensorGeometricProperties"
		message_id="4805" is_command="false">
		<description xml:space="preserve">This message is used to report 
            information about the geometric properties of a sensor with respect 
            to the vehicle. Three possible use cases are supported: 1.The 
            sensor has no knowledge of its geometric properties. 2.The sensor 
            is rigidly mounted to the vehicle and the position and orientation 
            with respect to the vehicle coordinate system is known. In this 
            case the position and orientation of the sensor is defined by a 
            position vector (x, y, z) and unit quaternion (d; a, b, c) which 
            specifies the axis and angle of rotation used to establish the 
            orientation of the sensor coordinate system with respect to the 
            vehicle coordinate system. 3.The sensor is mounted to a manipulator 
            in which the JAUS ID, joint number and the sensor offset are 
            described. In this case the position and orientation of the sensor 
            is defined by a position vector (x, y, z) and unit quaternion 
            (d; a, b, c) which specifies the axis and angle of rotation used 
            to establish the orientation of the sensor coordinate system with 
            respect to the link coordinate system.</description>
		<declared_header name="AppHeader"
			declared_type_ref="coreTypes.JAUSApplicationLayerHeader"/>
		<body name="Body">
			<list name="GeometricPropertiesList" optional="false">
				<count_field min_count="0" max_count="65535"
					field_type_unsigned="unsigned short integer"/>
				<sequence name="GeometricPropertiesSequence"
					optional="false">
					<declared_record name="SensorIdRec"
						declared_type_ref="environmentTypes.SensorIdRec"
						optional="false"/>
					<variant name="GeometricPropertiesVariant"
						optional="false">
						<vtag_field min_count="0" max_count="2"
							field_type_unsigned="unsigned byte"/>
						<variant name="NoGeometricPropertiesVariant"
							optional="false">
							<vtag_field min_count="0" max_count="0"
								field_type_unsigned="unsigned byte"/>
						</variant>
						<record name="StaticGeometricPropertiesRec"
							optional="false">
							<declared_array name="SensorPosition"
								declared_type_ref="environmentTypes.PositionVector"
								optional="false"/>
							<declared_array name="UnitQuaternion"
								declared_type_ref="environmentTypes.UnitQuaternion"
								optional="false"/>
						</record>
						<record
							name="ManipulatorGeometricPropertiesRec"
							optional="false">
							<fixed_field name="SubsystemID"
								optional="false" field_units="one"
								field_type="unsigned short integer"/>
							<fixed_field name="NodeID"
								optional="false" field_units="one"
								field_type="unsigned byte"/>
							<fixed_field name="ComponentID"
								optional="false" field_units="one"
								field_type="unsigned byte"/>
							<fixed_field name="JointNumber"
								optional="false" field_units="one"
								field_type="unsigned byte"/>
							<declared_array name="SensorPosition"
								declared_type_ref="environmentTypes.PositionVector"
								optional="false"/>
							<declared_array name="UnitQuaternion"
								declared_type_ref="environmentTypes.UnitQuaternion"
								optional="false"/>
						</record>
					</variant>
				</sequence>
			</list>
		</body>
		<footer name="Footer"/>
	</message_def>

	<message_def name="ReportVisualSensorCapabilities"
		message_id="4806" is_command="false">
		<description xml:space="preserve">This message is used to report the 
            sensorsâ€™ capabilities upon reciept of a Query Visual Sensor 
            Capabilities message. Capabilities include sensor properties, 
            values and ranges which can be modified by the Set Range Sensor 
            Configuration message.</description>
		<declared_header name="AppHeader"
			declared_type_ref="coreTypes.JAUSApplicationLayerHeader"/>
		<body name="Body">
			<list name="VisualSensorCapabilitiesList" optional="false">
				<count_field min_count="0" max_count="65535"
					field_type_unsigned="unsigned short integer"/>
				<record name="VisualSensorCapabilitiesRec"
					optional="false">
					<presence_vector
						field_type_unsigned="unsigned short integer"/>
					<declared_fixed_field name="SensorID"
						declared_type_ref="environmentTypes.SensorID"
						optional="false"/>
					<declared_variable_length_string name="SensorName"
						declared_type_ref="environmentTypes.SensorName"
						optional="false"/>
					<declared_bit_field name="SupportedStates"
						declared_type_ref="environmentTypes.SupportedSensorStates"
						optional="true"/>
					<bit_field name="ZoomModes" optional="true"
						field_type_unsigned="unsigned byte">
						<sub_field name="Mixed">
							<bit_range from_index="0" to_index="0"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="AnalogOnly">
							<bit_range from_index="1" to_index="1"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="DigitalOnly">
							<bit_range from_index="2" to_index="2"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="None">
							<bit_range from_index="3" to_index="3"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
					</bit_field>
					<bit_field name="FocusModes" optional="true"
						field_type_unsigned="unsigned byte">
						<sub_field name="AutoFocus">
							<bit_range from_index="0" to_index="0"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="ManualFocus">
							<bit_range from_index="1" to_index="1"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
					</bit_field>
					<bit_field name="WhiteBalance" optional="true"
						field_type_unsigned="unsigned byte">
						<sub_field name="Auto">
							<bit_range from_index="0" to_index="0"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Daylight">
							<bit_range from_index="1" to_index="1"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Cloudy">
							<bit_range from_index="2" to_index="2"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Shade">
							<bit_range from_index="3" to_index="3"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Tungsten">
							<bit_range from_index="4" to_index="4"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Flurescent">
							<bit_range from_index="5" to_index="5"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Flash">
							<bit_range from_index="6" to_index="6"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
					</bit_field>
					<bit_field name="ImagingModes" optional="true"
						field_type_unsigned="unsigned byte">
						<sub_field name="Color">
							<bit_range from_index="0" to_index="0"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Greyscale">
							<bit_range from_index="1" to_index="1"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Infrared">
							<bit_range from_index="2" to_index="2"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
						<sub_field name="Lowlight">
							<bit_range from_index="3" to_index="3"/>
							<value_set offset_to_lower_limit="false">
								<value_enum enum_const="Unsupported"
								enum_index="0"/>
								<value_enum enum_const="Supported"
								enum_index="1"/>
							</value_set>
						</sub_field>
					</bit_field>
					<bit_field name="ExposureModes" optional="true"
						field_type_unsigned="unsigned byte">
						<sub_field name="Auto">
							<bit_range from_index="0" to_index="0"/>
