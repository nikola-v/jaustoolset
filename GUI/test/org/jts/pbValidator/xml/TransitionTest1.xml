<?xml version="1.0" encoding="UTF-8"?>
<?oxygen RNGSchema="../../../../../resources/schema/JSIDL_Plus/jsidl_plus.rnc" type="compact"?>
<service_def xmlns="urn:jaus:jsidl:1.0" xmlns:ns1="urn:jaus:jsidl:1.1" id="urn:jts:TransitionTest1"
   name="TransitionTest1" version="1.0">
  <description xml:space="preserve">
     Target state of transition must be in the same state machine as the source state
  </description>
  <assumptions/> 
  <message_set>
    <input_set>
       <message_def name="Message1" message_id="0001" is_command="true">
          <description xml:space="preserve">
             A message
          </description>
          <header name="header"/>
          <body name="body"/>
          <footer name="footer"/>
       </message_def>
    </input_set>
    <output_set/>
  </message_set>
  <internal_events_set/>
  <protocol_behavior>
    <start state_machine_name="FSM" state_name="Ready"/>
    <state_machine name="FSM">
      <state name="Ready"/>
    </state_machine>
  </protocol_behavior>
</service_def>
