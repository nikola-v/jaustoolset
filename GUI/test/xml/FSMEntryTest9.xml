<?xml version="1.0" encoding="UTF-8"?>
<service_def xmlns="urn:jaus:jsidl:1.0" name="FSMTest" id="urn:JTS:CodeGenerator:ProtocolBehavior.FSMTest9" version="1.0">
  <description>transition to child state should include current state entry actions along with child's actions</description>
  <assumptions></assumptions>
  <message_set>
    <input_set></input_set>
    <output_set></output_set>
  </message_set>
  <internal_events_set></internal_events_set>
  <protocol_behavior is_stateless="true">
    <start state_machine_name="FSMEntryTest9" state_name="B"/>
    <state_machine name="FSMEntryTest9">
      <state name="B" initial_state="BB1">
      	<entry>
      		<action name ="EnterB">
      		</action>
      	</entry>
      	<state name="BB1">
      		<entry>
      		<action name="EnterBB1">
      		</action>
      		</entry>
			<transition name = "T1">
				<simple>
					<end_state state="B.BB2" />
				</simple>
				<action name="T1Action">
				</action>
			</transition>
      	</state>
      	<state name="BB2">
      		<entry>
      		<action name="EnterBB2">
      		</action>
      		</entry>
      	</state>
      </state>
    </state_machine>
  </protocol_behavior>  
</service_def>

