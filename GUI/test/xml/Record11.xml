<?xml version="1.0" encoding="UTF-8"?>
<ns1:service_set xmlns:ns1="urn:jaus:jsidl:plus" name="TestSet" id="urn:DeVivo:jaus_services:TestSet" version="0.1" xmlns="urn:jaus:jsidl:1.0">
  <description xml:space="preserve">The set of services to be tested</description>
  <service_def name="VariableLengthString1" id="urn:org.jts.test:VariableLengthString1" version="1.0" xmlns="urn:jaus:jsidl:1.0">
  	<description/>
  	<assumptions/>
  	<message_set>
  		<input_set>
  			<message_def name="MsgIn" is_command="true" message_id="0001">
  				<description xml:space="default"/>
          <header name="Header">
            <record name="HeaderRec" optional="false">
              <fixed_field name="MessageID" field_type="unsigned short integer" field_units="one"
                optional="false" interpretation="A two byte field to hold the message ID of a message"/>
            </record>          
          </header>
  				<body name="Body">
  					<record name="Record11" optional="false">
  						<variable_length_string name="VariableLengthString1" optional="false" interpretation="A               variable length string that has an upper bound of 5000 characters">
  							<count_field field_type_unsigned="unsigned short integer" min_count="0" max_count="5000"/>
  						</variable_length_string>
  					</record>
  				</body>
  				<footer name="EmptyFooter"/>
  			</message_def>
  		</input_set>
  		<output_set/>
  	</message_set>
  	<internal_events_set/>
  	<protocol_behavior>
  		<start state_machine_name="stateMachine" state_name="Ready"/>
  		<state_machine name="stateMachine">
  			<state name="Ready"/>
  		</state_machine>
  	</protocol_behavior>
  </service_def>
</ns1:service_set>